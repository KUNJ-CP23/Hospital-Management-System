@using System.Data;
@model System.Data.DataTable;

<div class="pagetitle">
    <h1>DoctorDepartment List</h1>
    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="index.html">Home</a></li>
            <li class="breadcrumb-item">DoctorDepartment</li>
            <li class="breadcrumb-item active">List</li>
        </ol>
    </nav>
</div><!-- End Page Title -->

<section class="section">
    <div class="row">
        <div class="col-lg-12">

            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">Datatables</h5>
                    <table>
                        <thead>
                            <tr>
                                <th>DoctorDepartmentID</th>
                                <th>DoctorName</th>
                                <th>DepartmentName</th>
                                <th>Created</th>
                                <th>Modified</th>
                                <th>UserName</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (DataRow dr in Model.Rows)
                            {
                                <tr>
                                    <td>@dr["DoctorDepartmentID"]</td>
                                    <td>@dr["DoctorName"]</td>
                                    <td>@dr["DepartmentName"]</td>
                                    <td>@dr["Created"]</td>
                                    <td>@dr["Modified"]</td>
                                    <td>@dr["UserName"]</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>
